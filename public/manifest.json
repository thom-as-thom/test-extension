{
    "manifest_version": 3,
    "name": "extension test",
    "version": "1.0.0",
    "icons": {
        "32": "ideame_51.png"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}